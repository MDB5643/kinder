	/* Custom Hourly - Specific site search results - Brenton Kelly - 6.12.18 */
    window.searchText = function() {
    	var csSearchID = '017762252505186220978:hnijvhl3o9o';
        var csKeyCodes = { 'tab': 9, 'enter': 13, 'esc': 27, 'space': 32, 'end': 35, 'home': 36, 'left': 37, 'up': 38, 'right': 39, 'down': 40 };
		var searchContainer = '#gb-search';
        var searchInput = '.gb-search-input, #gb-search-input-one';
        var searchButton = '.gb-search-button';
        var mobileSearchForm = '#rs-menu-search-form';
        var mobileSearchInput = '#rs-menu-search-input';
        var mobileSearchButton = '#rs-menu-search-button';
	
    	// DESKTOP SEARCH
        $(searchButton).removeAttr('onclick').unbind();
        $(searchContainer).on('click keydown', searchButton, function(e) {
            if(allyClick(e)) {
                e.preventDefault();
                window.location.href = '//www.google.com/cse?cx=' + csSearchID + '&q=' + $(searchInput).val() + '+more:pagemap:document-siteid:1151&ie=UTF-8';
                e.stopImmediatePropagation();
            }
        });
        $(searchContainer).on('keydown', searchInput, function(e) {
            if(e.keyCode == csKeyCodes.enter) {
                e.preventDefault();
                window.location.href = '//www.google.com/cse?cx=' + csSearchID + '&q=' + $(searchInput).val() + '+more:pagemap:document-siteid:1151&ie=UTF-8';
                e.stopImmediatePropagation();
            }
        });
        
        // MOBILE SEARCH
        if($(window).width() < 1024) {
            $(mobileSearchForm).unbind();
            $(mobileSearchButton).unbind();
            $(mobileSearchButton).click(function(e){
                e.preventDefault();
                window.location.href = '//www.google.com/cse?cx=' + csSearchID + '&q=' + $(mobileSearchInput).val() + '+more:pagemap:document-siteid:1151&ie=UTF-8';
                e.stopImmediatePropagation();
            });
            $(mobileSearchForm).submit(function(e){ 
                e.preventDefault();
                window.location.href = '//www.google.com/cse?cx=' + csSearchID + '&q=' + $(mobileSearchInput).val() + '+more:pagemap:document-siteid:1151&ie=UTF-8';
                e.stopImmediatePropagation();
            });
        }
        $(window).resize(function() {
            if($(window).width() < 1024) {
                $(mobileSearchForm).unbind();
            	$(mobileSearchButton).unbind();
            	$(mobileSearchButton).click(function(e){
                    e.preventDefault();
                    window.location.href = '//www.google.com/cse?cx=' + csSearchID + '&q=' + $(mobileSearchInput).val() + '+more:pagemap:document-siteid:1151&ie=UTF-8';
                    e.stopImmediatePropagation();
                });
                $(mobileSearchForm).submit(function(e){ 
                    e.preventDefault();
                    window.location.href = '//www.google.com/cse?cx=' + csSearchID + '&q=' + $(mobileSearchInput).val() + '+more:pagemap:document-siteid:1151&ie=UTF-8';
                    e.stopImmediatePropagation();
                });
            }
        });

        function allyClick(event) {
            if(event.type == 'click') {
                return true;
            } else if(event.type == 'keydown' && (event.keyCode == csKeyCodes.space || event.keyCode == csKeyCodes.enter)) {
                return true;
            } else {
                return false;
            }
        }
        
        $('.gb-search-input').focus(function() {
			$('#gb-search-globals-inner').addClass('search-active');
		
			if($(this).val() == 'Search...') {
				$(this).val('');
			}
		});
		$('.gb-search-input').blur(function() {
			$('#gb-search-globals-inner').removeClass('search-active');
		
			if($(this).val() == '') {
				$(this).val('Search...');
			}
		});
    }
});
